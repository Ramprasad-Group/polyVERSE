unfix           step19

fix             step20 all nvt temp 308 308 $(100.0*dt)
run             10000
unfix           step20

fix             step21 all npt temp 308 308 $(100.0*dt) iso 1 1 $(100.0*dt)
run             800000
unfix           step21

undump          dump_Equilibration


### NPT
reset_timestep  0

dump            dump_NPT all custom 10000 npt.lammpstrj id mol type q xs ys zs ix iy iz
restart         10000000 NPT.restart

fix             fNPT all npt temp 308 308 $(100.0*dt) iso 1 1 $(100.0*dt)
run             10000000
unfix           fNPT

undump          dump_NPT


### MSDMeasurement
