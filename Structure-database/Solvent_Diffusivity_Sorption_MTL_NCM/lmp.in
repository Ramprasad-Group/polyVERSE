unfix           step10

fix             step11 all nvt temp 308 308 $(100.0*dt)
run             100000
unfix           step11

fix             step12 all npt temp 308 308 $(100.0*dt) iso 24673.0815 24673.0815 $(100.0*dt)
run             5000
unfix           step12

fix             step13 all nvt temp 600 600 $(100.0*dt)
run             5000
unfix           step13

fix             step14 all nvt temp 308 308 $(100.0*dt)
run             10000
unfix           step14

fix             step15 all npt temp 308 308 $(100.0*dt) iso 4934.616300000001 4934.616300000001 $(100.0*dt)
run             5000
unfix           step15

fix             step16 all nvt temp 600 600 $(100.0*dt)
run             5000
unfix           step16

fix             step17 all nvt temp 308 308 $(100.0*dt)
